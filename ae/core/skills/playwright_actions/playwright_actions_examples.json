{
  "examples": {
    "click_actions": [
      {
        "action": "click",
        "selector": {
          "type": "attributeValueSelector",
          "attribute": "id",
          "value": "submit-button"
        },
        "wait_before_execution": 1.0,
        "description": "Click the submit button using ID selector"
      },
      {
        "action": "click",
        "selector": {
          "type": "attributeValueSelector",
          "attribute": "class",
          "value": "btn-primary"
        },
        "description": "Click the primary button using class selector"
      },
      {
        "action": "click",
        "selector": {
          "type": "attributeValueSelector",
          "attribute": "name",
          "value": "login-btn"
        },
        "description": "Click the login button using name selector"
      },
      {
        "action": "click",
        "selector": {
          "type": "tagContainsSelector",
          "value": "Submit"
        },
        "description": "Click the button containing 'Submit' text"
      },
      {
        "action": "click",
        "selector": {
          "type": "xpathSelector",
          "value": "//button[@type='submit']"
        },
        "description": "Click the submit button using XPath"
      }
    ],
    
    "drag_and_drop_actions": [
      {
        "action": "drag_and_drop",
        "source_selector": {
          "type": "attributeValueSelector",
          "attribute": "id",
          "value": "draggable-item"
        },
        "target_selector": {
          "type": "attributeValueSelector",
          "attribute": "class",
          "value": "drop-zone"
        },
        "wait_before_execution": 0.5,
        "description": "Drag item to drop zone using ID and class selectors"
      },
      {
        "action": "drag_and_drop",
        "source_selector": {
          "type": "xpathSelector",
          "value": "//div[@class='draggable']"
        },
        "target_selector": {
          "type": "xpathSelector",
          "value": "//div[@class='dropzone']"
        },
        "description": "Drag and drop using XPath selectors"
      }
    ],
    
