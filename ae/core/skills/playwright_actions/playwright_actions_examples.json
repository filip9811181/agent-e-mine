  "usage_notes": {
    "selector_rules": {
      "attributeValueSelector": {
        "id": "Must be prefixed with # when converted to Playwright selector",
        "class": "Must be prefixed with . when converted to Playwright selector",
        "name": "Will be converted to [name='value'] format"
      },
      "tagContainsSelector": {
        "value": "Will be converted to text=value format for Playwright"
      },
      "xpathSelector": {
        "value": "Must start with / or // and will be used directly by Playwright"
      }
    },
    "best_practices": [
      "Use attributeValueSelector for elements with stable IDs or names",
      "Use tagContainsSelector for buttons and links with descriptive text",
      "Use xpathSelector for complex element selection scenarios",
      "Always include descriptions for better action documentation",
      "Use wait_before_execution when actions need timing considerations"
    ]
  }
}
