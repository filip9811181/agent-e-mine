  "best_practices": [
    "Always include a description for better action documentation",
    "Use appropriate selector types for different scenarios",
    "Prefer attributeValueSelector for stable elements (id, name)",
    "Use tagContainsSelector for text-based selection",
    "Use xpathSelector for complex element relationships",
    "Set appropriate wait times for page interactions",
    "Handle errors gracefully with proper validation",
    "Use meaningful file paths for screenshots",
    "Group related actions into workflows",
    "Test selectors before using them in automation"
  ],
  
  "error_handling": {
    "common_errors": [
      "Element not found - selector may be invalid or element not loaded",
      "Timeout errors - increase wait times or check page state",
      "Invalid selector format - ensure proper JSON structure",
      "Missing required fields - check action type requirements",
